<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ACProgressBarHorizontal" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressTint">@color/cisco_green</item>
        <item name="android:progressBackgroundTint">@color/cisco_dark_blue</item>
    </style>
    <style name="CSCTheme" parent="@style/Theme.Material3.DayNight">
        <item name="android:actionBarStyle">@style/CSCActionBar</item>
        <item name="android:statusBarColor">@color/status_blue_wash_3</item>
        <item name="actionBarStyle">@style/CSCActionBar</item>
        <item name="actionOverflowMenuStyle">@style/RoundedPopupMenu</item>
        <item name="colorAccent">@color/cisco_blue</item>
        <item name="colorControlHighlight">@color/status_blue_wash_3</item>
        <item name="colorPrimary">@color/cisco_blue</item>
        <item name="statusBarBackground">@color/status_blue_wash_3</item>
    </style>
    <style name="CSCThemeNoActionBar" parent="@style/Theme.Material3.DayNight.NoActionBar">
        <item name="android:statusBarColor">?android:colorBackground</item>
        <item name="colorAccent">@color/cisco_green</item>
        <item name="colorControlHighlight">@color/status_blue_wash_3</item>
        <item name="colorPrimary">@color/cisco_blue</item>
        <item name="popupMenuStyle">@style/RoundedPopupMenu</item>
        <item name="toolbarStyle">@style/CSCToolbar</item>
    </style>
    <style name="default_style_button" parent="@style/Widget.Material3.Button">
        <item name="android:layout_margin">20.0dip</item>
        <item name="android:maxLines">2</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="backgroundTint">@color/cisco_blue_wash_1</item>
    </style>
</resources>
